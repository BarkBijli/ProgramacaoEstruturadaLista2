#include <stdio.h>

int maiorNumero(int numero1, int numero2) {
    if (numero1 > numero2) {
        return numero1;
    } else {
        return numero2;
    }
}

int main() {
    int num1, num2;
    
    printf("Digite o primeiro numero: ");
    scanf("%d", &num1);
    
    printf("Digite o segundo numero: ");
    scanf("%d", &num2);
    
    int resultado = maiorNumero(num1, num2);
    
    printf("O maior numero entre %d e %d e %d\n", num1, num2, resultado);
    
    return 0;
}
